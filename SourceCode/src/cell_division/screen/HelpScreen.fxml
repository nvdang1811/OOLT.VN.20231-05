<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <AnchorPane prefHeight="430.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" layoutX="370.0" layoutY="14.0" text="Tutorial">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <ImageView fx:id="imgView" fitHeight="328.0" fitWidth="535.0" layoutX="165.0" layoutY="68.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/tutorialMain(1).png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <Button fx:id="btnReturn" mnemonicParsing="false" onAction="#btnReturnPressed" text="Return" BorderPane.alignment="CENTER">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Button>
   </bottom>
   <left>
      <Button fx:id="btnPrevious" mnemonicParsing="false" onAction="#btnPreviousPressed" prefHeight="31.0" prefWidth="61.0" text="Previous" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Button fx:id="btnNext" mnemonicParsing="false" onAction="#btnNextPressed" prefHeight="31.0" prefWidth="61.0" text="Next" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
