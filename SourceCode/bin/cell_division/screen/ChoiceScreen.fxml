<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <AnchorPane prefHeight="300.0" prefWidth="800.0">
         <children>
            <VBox alignment="CENTER" layoutX="5.0" layoutY="78.0" prefHeight="428.0" prefWidth="790.0" spacing="100.0">
               <children>
                  <Button fx:id="btnViewComponents" mnemonicParsing="false" onAction="#btnViewComponentsPressed" prefHeight="100.0" prefWidth="200.0" text="View Components">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="100.0">
                     <children>
                        <Button fx:id="btnViewProcess1" mnemonicParsing="false" onAction="#viewProcess1" prefHeight="100.0" prefWidth="200.0" text="View Process 1">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                        <Button fx:id="btnViewProcess2" mnemonicParsing="false" onAction="#viewProcess2" prefHeight="100.0" prefWidth="200.0" text="View Process 2">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
                  <Button fx:id="btnReturn" mnemonicParsing="false" onAction="#btnReturnPreessed" prefHeight="100.0" prefWidth="200.0" text="Return">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </center>
   <top>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblCellType" text="Cell type chosen" textFill="#0f27ff">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </top>
   <bottom>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
